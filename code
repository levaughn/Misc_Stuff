

#include <iostream>
using namespace std;

float x, y, z, q, B, N;


float  num_func( float x, float y, float z, float q)
	{
	

	float N;
	N = ((x * (y * z) / q));
	
	return(N);
}

int main()
{
	cout << "Enter nums...."<<endl;
	cin >> x >> y >> z >> q;
	
	
	
	B =  num_func(x,y,z,q);
	if (q == 0.0)
		cout << "foo!";
	else
	{
		cout << "Ans is..." << B;
	}
	
	return 0;
}
